<%= simple_form_for product, url: products_path, method: :post  do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :balance %>

  <%= f.button :submit, "inline-block px-5 py-3 font-medium text-white bg-blue-600 rounded-lg cursor-pointer"%>
<% end %>
